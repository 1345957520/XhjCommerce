@model IList<CategoryModel>

@using Chj.Web.Models.Catalog;
@if (Model.Count > 0)
{
    var cat_i = 0;
    foreach (var itemOne in Model)
    {
        <div class="recommended_items"><!--recommended_items-->
            <div>
                <div>
                    <span><a href="@Url.RouteUrl("Category", new { SeName = itemOne.SeName })">@itemOne.Name</a></span>
                </div>
                <div class="container ">
                    @foreach (var item in itemOne.SubCategories)
{
    <div class=".col-md-6">
        <span><a href="@Url.RouteUrl("Category", new { SeName = item.SeName })">@item.Name</a></span>
    </div>
}
                </div>
            </div>
            <div>

            </div>
    </div>
    }
    


   


}






